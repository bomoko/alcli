define('ALCLI_BIN_PATH', __FILE__);

if (is_file($autoload = getcwd() . '/vendor/autoload.php')) {
  require $autoload;
}


use Symfony\Component\Yaml\Yaml;
use Symfony\Component\Console\Application;
use AmazeeLabs\alcli\ListIssuesCommand;
use AmazeeLabs\alcli\ListUserWorklogsCommand;

$application = new Application();

$application->add(new ListIssuesCommand());
$application->add(new ListUserWorklogsCommand());

$application->run();
